timePicker.timeTo\"\r\n                  label=\"結束時間\"\r\n                  prepend-icon=\"access_time\"\r\n                  readonly\r\n                  v-on=\"on\"\r\n                  color=\"#FFF\"\r\n                ></v-text-field>\r\n              </template>\r\n              <v-time-picker\r\n                v-if=\"timePicker.modal2\"\r\n                v-model=\"timePicker.timeTo\"\r\n                full-width\r\n                format=\"24hr\"\r\n                color=\"#A78E70\"\r\n              >\r\n                <v-spacer></v-spacer>\r\n                <v-btn flat color=\"primary\" @click=\"timePicker.modal2 = false\">Cancel</v-btn>\r\n                <v-btn\r\n                  flat\r\n                  color=\"primary\"\r\n                  @click=\"$refs.dialog2.save(timePicker.timeTo),timeCaculate()\"\r\n                >OK</v-btn>\r\n              </v-time-picker>\r\n            </v-dialog>\r\n          </v-flex>\r\n        </v-layout>\r\n        <div class=\"to\" v-else>{{timePicker.timeFrom+\" 到 \"+timePicker.timeTo}}</div>\r\n      </div>\r\n      <div class=\"total\" v-if=\"detial.status\">\r\n        <div\r\n          v-if=\"total.status\"\r\n          :style=\"{color: 'white', transition: ' 1s'}\"\r\n        >{{\"共計：\"+total.value+\"分鐘\"}}</div>\r\n        <div v-else :style=\"{color: total.massageColor, transition: ' 1s'}\">{{total.massage}}</div>\r\n      </div>\r\n      <div v-if=\"finishText.status\">\r\n        <h3>{{finishText.title}}</h3>\r\n        <p>{{finishText.name}}</p>\r\n      </div>\r\n      <div v-if=\"record.status\">\r\n        <table class=\"myTable\">\r\n          <tr>\r\n            <th>編號</th>\r\n            <th>日期</th>\r\n            <th>時間</th>\r\n            <th>時數</th>\r\n            <th>審核人</th>\r\n            <th>審核狀態</th>\r\n          </tr>\r\n          <tr v-for=\"(record,index) in recordData\" :key=\"index\">\r\n            <td>{{index+1}}</td>\r\n            <td class=\"myTd\">{{record.date}}</td>\r\n   