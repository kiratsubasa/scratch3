rue\r\n            },\r\n            home: \"HOME\",\r\n            finishText: {\r\n            title: \"審核流程\",\r\n            name: \"主任：許素朱\",\r\n            status: false\r\n            },\r\n            record: {\r\n            status: false\r\n            },\r\n            recordData: [\r\n            {\r\n                date: \"2019-06-30\",\r\n                from: \"17:30\",\r\n                to: \"18:30\",\r\n                total: \"60\",\r\n                name: \"許素朱\",\r\n                state: \"審核完成\"\r\n            }\r\n            ],\r\n            color: {\r\n            bgc: \"#333333\",\r\n            but: \"#A78E70\"\r\n            },\r\n            // inject: ['reload']\r\n        }\r\n    },\r\n    \r\n    methods: {\r\n        buttonClick: function() {\r\n            if (this.total.value > 0) {\r\n                this.nextPage();\r\n            }\r\n        },\r\n        nextPage: function() {\r\n            if (this.datePicker.status) {\r\n                this.button.text = \"送出\";\r\n                this.color.bgc = \"#A78E70\";\r\n                this.color.but = \"#333333\";\r\n                this.datePicker.status = false;\r\n                this.timePicker.status = false;\r\n                this.addData();\r\n            } else if (this.finishText.status) {\r\n                this.title = \"加班紀錄\";\r\n                this.finishText.status = false;\r\n                this.button.status = false;\r\n                this.record.status = true;\r\n            } else {\r\n                this.title = \"加班申請完成\";\r\n                this.button.text = \"查看加班紀錄\";\r\n                this.button.shape = \"recButton\";\r\n                this.detial.status = false;\r\n                this.finishText.status = true;\r\n            }\r\n        },\r\n        timeCaculate: function() {\r\n          var timeFromAry;\r\n          var timeToAry;\r\n        if (this.timePicker.timeFrom && this.timePicker.timeTo) {\r\n            \r\n            timeFromAry = this.timePicker.timeFrom.split(\":\");\r\n            timeToAry = this.timePicker.timeTo.split(\":\");\r\n            var total;\r\n            if (timeToAry[1] < timeFromAry[1]) {\r\n            timeToAry[0] -= 1;\r\n            timeToAry[1] = parseInt(timeToAry[1]);\r\n            timeToAry[1] += 60;\r\n            }\r\n            total = timeToAry[1] - timeFromAry[1];\r\n            if (timeToAry[0] < timeFromAry[0]) {\r\n            this.total.massage = \"請輸入正確時間\";\r\n            this.total.massageColor = \"red\";\r\n            this.total.status = false;\r\n            } else {\r\n            total += (timeToAry[0] - timeFromAry[0]) * 60;\r\n            this.total.value = total;\r\n            this.total.status = true;\r\n            }\r\n        }\r\n        },\r\n        addData: function() {\r\n        this.recordData.push({\r\n            date: this.datePicker.date,\r\n            from: this.timePicker.timeFrom,\r\n            to: this.timePicker.timeTo,\r\n            total: this.total.value,\r\n            name: \"許素朱\",\r\n            state: \"審核未完成\"\r\n        },\r\n      );\r\n    },\r\n    \r\n    reload: function(){\r\n            this.status = true;\r\n            this.datePicker.menu= false;\r\n            this.datePicker.modal= false;\r\n            this.datePicker.menu2= false;\r\n            this.datePicker.status= true;\r\n            this.timePicker.timeFrom = null;\r\n            this.timePicker.timeTo=null;\r\n            this.timePicker.menu2= false;\r\n            this.timePicker.modal= false;\r\n            this.timePicker.modal2= false;\r\n            this.timePicker.status= true;\r\n            this.title= \"加班申請\";\r\n            this.total.value = 0;\r\n            this.total.massage= \"請選擇時段\";\r\n            this.total.massageColor= \"white\";\r\n            this.total.status= false;\r\n            this.total.value= 0;\r\n            this.detial.status = true;\r\n            this.button.text = \"申請\";\r\n            this