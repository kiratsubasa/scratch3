n                v-model=\"datePicker.modal\"\r\n                :return-value.sync=\"datePicker.date\"\r\n                persistent\r\n                lazy\r\n                full-width\r\n                width=\"290px\"\r\n              >\r\n                <template v-slot:activator=\"{ on }\">\r\n                  <v-text-field\r\n                    dark\r\n                    v-model=\"datePicker.date\"\r\n                    label=\"請選擇日期\"\r\n                    prepend-icon=\"event\"\r\n                    readonly\r\n                    color=\"white\"\r\n                    v-on=\"on\"\r\n                    single-line\r\n                  ></v-text-field>\r\n                </template>\r\n                <v-date-picker v-model=\"datePicker.date\" scrollable locale=\"zh-cn\" color=\"#A78E70\">\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn flat color=\"primary\" @click=\"datePicker.modal = false\">Cancel</v-btn>\r\n                  <v-btn flat color=\"primary\" @click=\"$refs.dialog3.save(datePicker.date)\">OK</v-btn>\r\n                </v-date-picker>\r\n              </v-dialog>\r\n            </v-flex>\r\n            <!--         <template>\r\n          <div>\r\n            <v-date-picker v-model=\"picker\" :landscape=true locale=\"zh-cn\" color=\"#A78E70\"></v-date-picker>\r\n          </div>      \r\n            </template>-->\r\n          </v-layout>\r\n        </v-content>\r\n        <div class=\"date\" v-else>{{datePicker.date}}</div>\r\n      </div>\r\n\r\n      <div class=\"from\" v-if=\"detial.status\">\r\n        時間：\r\n        <v-layout row wrap justify-center v-if=\"timePicker.status\">\r\n          <v-flex xs12 sm3 order-sm1>\r\n            <v-dialog\r\n              ref=\"dialog\"\r\n              v-model=\" timePicker.modal\"\r\n              :return-value.sync=\"timePicker.timeFrom\"\r\n              persistent\r\n              lazy\r\n              full-width\r\n              width=\"290px\"\r\n            >\r\n              <template v-slot:activator=\"{ on }\">\r\n                <v-text-field\r\n                  dark\r\n                  v-model=\"timePicker.timeFrom\"\r\n                  label=\"起始時間\"\r\n                  prepend-icon=\"access_time\"\r\n                  readonly\r\n                  v-on=\"on\"\r\n                  color=\"#FFF\"\r\n                ></v-text-field>\r\n              </template>\r\n              <v-time-picker\r\n                v-if=\"timePicker.modal\"\r\n                v-model=\"timePicker.timeFrom\"\r\n                full-width\r\n                format=\"24hr\"\r\n                color=\"#A78E70\"\r\n              >\r\n                <v-spacer></v-spacer>\r\n                <v-btn flat color=\"primary\" @click=\"timePicker.modal = false\">Cancel</v-btn>\r\n                <v-btn\r\n                  flat\r\n                  color=\"primary\"\r\n                  @click=\"$refs.dialog.save(timePicker.timeFrom),timeCaculate()\"\r\n                >OK</v-btn>\r\n              </v-time-picker>\r\n            </v-dialog>\r\n          </v-flex>\r\n          <v-flex xs12 sm3 order-sm2>\r\n            <v-dialog\r\n              ref=\"dialog2\"\r\n              v-model=\" timePicker.modal2\"\r\n              :return-value.sync=\"timePicker.timeTo\"\r\n              persistent\r\n              lazy\r\n              full-width\r\n              width=\"290px\"\r\n            >\r\n              <template v-slot:activator=\"{ on }\">\r\n                <v-text-field\r\n                  dark\r\n                  v-model=\"timePicker.timeTo\"\r\n                  label=\"結束時間\"\r\n                  prepend-icon=\"access_time\"\r\n                  readonly\r\n                  v-on=\"on\"\r\n                  color=\"#FFF\"\r\n                ></v-text-field>\r\n              </template>\r\n              <v-time-picker\r\n                v-if=\"timePicker.modal2\"\r\n                v-model=\"timePicker.timeTo\"\r\n                full-width\r\n                format=\"24hr\"\r\n                color=\"#A78E70\"\r\n              >\r\n                <v-spacer></v-spacer>\r\n                <v-btn flat color=\"primary\" @click=\"timePicker.modal2 = false\">Cancel</v-btn>\r\n                <v-btn\r\n                  flat\r\n                  color=\"primary\"\r\n                  @click=\"$refs.dialog2.save(timePicker.timeTo),timeCaculate()\"\r\n                >OK</v-btn>\r\n              </v-time-picker>\r\n            </v-dialog>\r\n          </v-flex>\r\n        </v-layout>\r\n        <div class=\"to\" v-else>{{timePicker.timeFrom+\" 到 \"+timePicker.timeTo}}</div>\r\n      </div>\r\n      <div class=\"total\" v-if=\"detial.status\">\r\n        <div\r\n          v-if=\"total.status\"\r\n          :style=\"{color: 'white', transition: ' 1s'}\"\r\n        >{{\"共計：\"+total.value+\"分鐘\"}}</div>\r\n        <div v-else :style=\"{color: total.massageColor, transition: ' 1s'}\">{{total.massage}}</div>\r\n      </div>\r\n      <div v-if=\"finishText.status\">\r\n        <h3>{{finishText.title}}</h3>\r\n        <p>{{finishText.name}}</p>\r\n      </div>\r\n      <div v-if=\"record.status\">\r\n        <table class=\"myTable\">\r\n          <tr>\r\n            <th>編號</th>\r\n            <th>日期</th>\r\n            <th>時間</th>\r\n            <th>時數</th>\r\n            <th>審核人</th>\r\n            <th>審核狀態</th>\r\n          </tr>\r\n          <tr v-for=\"(record,index) in recordData\" :key=\"index\">\r\n            <td>{{index+1}}</td>\r\n            <td class=\"myTd\">{{record.date}}</td>\r\n            <td class=\"myTd\">{{record.from}}-{{record.to}}</td>\r\n            <td class=\"myTd\">{{record.total}}</td>\r\n            <td class=\"myTd\">{{record.name}}</td>\r\n            <td class=\"myTd\">{{record.state}}</td>\r\n          </tr>\r\n        </table>\r\n      </div>\r\n    </div>\r\n    <div\r\n      :class=\"button.shape\"\r\n      v-on:click=\"buttonClick()\"\r\n      :style=\"{'background-color':color.but}\"\r\n      v-if=\"button.status\"\r\n    >{{button.text}}</div>\r\n    </v-container >\r\n  </div>\r\n  \r\n</template>\r\n\r\n<script>\r\nimport 'material-design-icons-iconfont/dist/material-design-icons.css' \r\n// var timeFromAry, timeToAry;\r\n\r\nexport default {\r\n    data(){\r\n        return{\r\n            drawer: false,\r\n            group: null,\r\n            menuItems: [\r\n            {title: \"休假申請\",link: '/leave/Picker'},\r\n            {title: \"打卡紀錄\",link: '/recording'},\r\n            {title: \"補休累計時數\",link: '/compensatoryTime'},\r\n            {title: \"職務代理人設定\",link: '/agent'}\r\n            ],\r\n            status: true,\r\n            datePicker: {\r\n            date: new Date().toISOString().substr(0, 10),\r\n            menu: false,\r\n            modal: false,\r\n            menu2: false,\r\n            status: true\r\n            },\r\n            timePicker: {\r\n            timeFrom: null,\r\n            timeTo: null,\r\n            menu2: false,\r\n            modal: false,\r\n            modal2: false,\r\n            status: true\r\n            },\r\n            title: \"加班申請\",\r\n            myMenu: {\r\n            menuOpen: false\r\n            },\r\n            total: {\r\n            value: 0,\r\n            massage: \"請選擇時段\",\r\n            massageColor: \"white\",\r\n            status: false\r\n            },\r\n            detial: {\r\n            status: true\r\n            },\r\n            button: {\r\n            text: \"申請\",\r\n            shape: \"circleButton\",\r\n            status: true\r\n            },\r\n            home: \"HOME\",\r\n            finishText: {\r\n            title: \"審核流程\",\r\n            name: \"主任：許素朱\",\r\n            status: false\r\n            },\r\n            record: {\r\n            status: false\r\n            },\r\n            recordData: [\r\n            {\r\n                date: \"2019-06-30\",\r\n                from: \"17:30\",\r\n                to: \"18:30\",\r\n                total: \"60\",\r\n                name: \"許素朱\",\r\n                state: \"審核完成\"\r\n            }\r\n            ],\r\n            color: {\r\n            bgc: \"#333333\",\r\n            but: \"#A78E70\"\r\n            },\r\n            // inject: ['reload']\r\n        }\r\n    },\r\n    \r\n    methods: {\r\n        buttonClick: function() {\r\n            if (this.total.value > 0) {\r\n                this.nextPage();\r\n            }\r\n        },\r\n        nextPage: function() {\r\n            if (this.datePicker.status) {\r\n                this.button.text = \"送出\";\r\n                this.color.bgc = \"#A78E70\";\r\n                this.color.but = \"#333333\";\r\n                this.datePicker.status = false;\r\n                this.timePicker.status = false;\r\n                this.addData();\r\n            } else if (this.finishText.status) {\r\n                this.title = \"加班紀錄\";\r\n                this.finishText.status = false;\r\n                this.button.status = false;\r\n                this.record.status = true;\r\n            } else {\r\n                this.title = \"加班申請完成\";\r\n                this.button.text = \"查看加班紀錄\";\r\n                this.button.shape = \"recButton\";\r\n                this.detial.status = false;\r\n                this.finishText.status = true;\r\n            }\r\n        },\r\n        timeCaculate: function() {\r\n          var timeFromAry;\r\n          var timeToAry;\r\n        if (this.timePicker.timeFrom && this.timePicker.timeTo) {\r\n            \r\n            timeFromAry = this.timePicker.timeFrom.split(\":\");\r\n            timeToAry = this.timePicker.timeTo.split(\":\");\r\n            var total;\r\n            if (timeToAry[1] < timeFromAry[1]) {\r\n            timeToAry[0] -= 1;\r\n            timeToAry[1] = parseInt(timeToAry[1]);\r\n            timeToAry[1] += 60;\r\n            }\r\n            total = timeToAry[1] - timeFromAry[1];\r\n            if (timeToAry[0] < timeFromAry[0]) {\r\n            this.total.massage = \"請輸入正確時間\";\r\n            this.total.massageColor = \"red\";\r\n            this.total.status = false;\r\n            } else {\r\n            total += (timeToAry[0] - timeFromAry[0]) * 60;\r\n            this.total.value = total;\r\n            this.total.status = true;\r\n            }\r\n        }\r\n        },\r\n        addData: function() {\r\n        this.recordData.push({\r\n            date: this.datePicker.date,\r\n            from: this.timePicker.timeFrom,\r\n            to: this.timePicker.timeTo,\r\n            total: this.total.value,\r\n            name: \"許素朱\",\r\n            state: \"審核未完成\"\r\n        },\r\n      );\r\n    },\r\n    \r\n    reload: function(){\r\n            this.status = true;\r\n            this.datePicker.menu= false;\r\n            this.datePicker.modal= false;\r\n            this.datePicker.menu2= false;\r\n            this.datePicker.status= true;\r\n            this.timePicker.timeFrom = null;\r\n            this.timePicker.timeTo=null;\r\n            this.timePicker.menu2= false;\r\n            this.timePicker.modal= false;\r\n            this.timePicker.modal2= false;\r\n            this.timePicker.status= true;\r\n            this.title= \"加班申請\";\r\n            this.total.value = 0;\r\n            this.total.massage= \"請選擇時段\";\r\n            this.total.massageColor= \"white\";\r\n            this.total.status= false;\r\n            this.total.value= 0;\r\n            this.detial.status = true;\r\n            this.button.text = \"申請\";\r\n            this.button.shape= \"circleButton\";\r\n            this.button.status = true;\r\n            this.finishText.status = false;\r\n            this.record.status = false;\r\n            this.color.bgc=\"#333\";\r\n            this.color.but=\"#A78E70\";\r\n    }\r\n    \r\n    },\r\n    watch: {\r\n      group () {\r\n        this.drawer = false\r\n      },\r\n    },\r\n  };\r\n\r\n</script>\r\n<style>\r\n .myTitle, .detial, .to, .myTable, .myTd, .recButton, .circleButton {\r\n  text-align: center;\r\n  color: #eee;\r\n  transition: 0.3s;\r\n}\r\n\r\n.myContaier{\r\n    background-color: #fff;\r\n    margin: 100%;\r\n}\r\n\r\n.myTitle {\r\n  position: relative;\r\n  top: 10%;\r\n}\r\n\r\n.detial {\r\n  position: relative;\r\n  top: 10%;\r\n  margin: 50px;\r\n  font-size: 120%;\r\n}\r\n\r\n.to, .date {\r\n  display: inline;\r\n  margin: 5px;\r\n}\r\n\r\n.myTable {\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.myTd {\r\n  padding: 15px;\r\n}\r\n\r\n.circleButton {\r\n  background-color: #A78E70;\r\n  border: none;\r\n  padding: 25px;\r\n  font-size: 30px;\r\n  height: 150px;\r\n  width: 150px;\r\n  margin: 0 auto;\r\n  border-radius: 50%;\r\n  line-height: 100px;\r\n  transition: 0.6s;\r\n  position: relative;\r\n  top: 10%;\r\n  cursor: pointer;\r\n}\r\n.circleButton:hover {\r\n  box-shadow: 2px 2px 2px rgba(51, 51, 102, 0.6), 4px 4px 6px rgba(51, 51, 102, 0.4), 6px 6px 12px rgba(51, 51, 102, 0.4);\r\n}\r\n\r\n.recButton {\r\n  border: none;\r\n  padding: 25px;\r\n  font-size: 20px;\r\n  width: 175px;\r\n  border-radius: 5%;\r\n  line-height: 30px;\r\n  margin: 0 auto;\r\n  transition: 0.6s;\r\n  position: relative;\r\n  top: 10%;\r\n  cursor: pointer;\r\n}\r\n.recButton:hover {\r\n  box-shadow: 2px 2px 2px rgba(51, 51, 102, 0.6), 4px 4px 6px rgba(51, 51, 102, 0.4), 6px 6px 12px rgba(51, 51, 102, 0.4);\r\n}\r\n\r\n\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}